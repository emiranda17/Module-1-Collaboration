    "#Binary Search\n",
    "class Solution:\n",
    "    def binarysearch(self, arr, k):\n",
    "        left = 0\n",
    "        right = len(arr) - 1\n",
    "        result = -1\n",
    "        while left <= right:\n",
    "            mid = (left + right) // 2\n",
    "            if arr[mid] == k:\n",
    "                result = mid\n",
    "                right = mid - 1\n",
    "                \n",
    "            elif arr[mid] < k:\n",
    "                    left = mid + 1\n",
    "            else:\n",
    "                right = mid - 1\n",
    "        return result\n",
               "
   ]
